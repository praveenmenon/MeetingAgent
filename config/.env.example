# Notion Configuration
NOTION_TOKEN=your_notion_integration_token_here
DATABASE_ID=your_meetings_database_id_here
TASKS_DATABASE_ID=your_tasks_database_id_here

# AI Services
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional Settings
DEFAULT_ASSIGNEE=Your Name

# Memory Configuration (Optional)
MEM0_API_KEY=your_mem0_api_key_here
DEFAULT_USER_ID=your_unique_user_id
MEMORY_ENABLED=true

# Async Processing (Optional)
ENABLE_ASYNC_PROCESSING=false
REDIS_URL=redis://localhost:6379

# AI Model Configuration Overrides (Optional)
# Summarization (Conservative, accurate)
# AI_SUMMARIZATION_TEMPERATURE=0.3
# AI_SUMMARIZATION_MAX_TOKENS=2000
# AI_SUMMARIZATION_MODEL=gpt-4o

# Brief Descriptions (Very focused)
# AI_BRIEF_DESCRIPTION_TEMPERATURE=0.2
# AI_BRIEF_DESCRIPTION_MAX_TOKENS=150

# Q&A (More conversational)
# AI_QA_ANSWERING_TEMPERATURE=0.5
# AI_QA_ANSWERING_MAX_TOKENS=1000

# Task Suggestions (Creative but practical)
# AI_TASK_SUGGESTION_TEMPERATURE=0.6
# AI_TASK_SUGGESTION_MAX_TOKENS=1500

# Similarity Checking (Analytical)
# AI_SIMILARITY_CHECK_TEMPERATURE=0.1
# AI_SIMILARITY_CHECK_MAX_TOKENS=200
# AI_SIMILARITY_CHECK_MODEL=claude-3-5-sonnet-20240620

# Creative Tasks
# AI_CREATIVE_WRITING_TEMPERATURE=0.8
# AI_CREATIVE_WRITING_MAX_TOKENS=2000

# Rate Limiting Configuration (Optional)
# RATE_LIMIT_MAX_RETRIES=5
# RATE_LIMIT_BASE_DELAY=1.0
# RATE_LIMIT_MAX_DELAY=60.0
# RATE_LIMIT_EXPONENTIAL_BASE=2.0
# RATE_LIMIT_JITTER=true
# RATE_LIMIT_RATE_LIMIT_DELAY=60.0
# RATE_LIMIT_QUOTA_EXCEEDED_DELAY=3600.0